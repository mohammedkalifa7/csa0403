#include <stdio.h>
#include <string.h>

int main() {
    char filename[20], pattern[20];
    char data[] = "UNIX File Management Program";

    printf("Enter filename: ");
    scanf("%s", filename);

    printf("Enter search pattern: ");
    scanf("%s", pattern);

    if (strstr(data, pattern)) {
        printf("%s\n", data);
    }

    return 0;
}
