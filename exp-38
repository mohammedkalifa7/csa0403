#include <stdio.h>
#include <stdlib.h>

int main() {
    int req[10], n, head, i, seek = 0;

    printf("Enter number of requests: ");
    scanf("%d", &n);

    printf("Enter request queue: ");
    for (i = 0; i < n; i++)
        scanf("%d", &req[i]);

    printf("Enter head position: ");
    scanf("%d", &head);

    seek = abs(199 - head) + 199;
    printf("Total Seek Time = %d\n", seek);
    return 0;
}
