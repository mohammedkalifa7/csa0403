#include <stdio.h>

int main() {
    int pages[20], frame[3], n = 6;
    int i, j, k, faults = 0;

    printf("Enter pages: ");
    for (i = 0; i < n; i++) scanf("%d", &pages[i]);

    for (i = 0; i < 3; i++) frame[i] = -1;

    for (i = 0; i < n; i++) {
        int found = 0;
        for (j = 0; j < 3; j++)
            if (frame[j] == pages[i]) found = 1;

        if (!found) {
            frame[i % 3] = pages[i];
            faults++;
        }
    }
    printf("Page Faults = %d\n", faults);
    return 0;
}
