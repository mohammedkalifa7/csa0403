#include <stdio.h>

int main() {
    int pages[20], frame[3], time[3], n = 6;
    int i, j, pos, faults = 0, counter = 0;

    printf("Enter pages: ");
    for (i = 0; i < n; i++) scanf("%d", &pages[i]);

    for (i = 0; i < 3; i++) frame[i] = -1;

    for (i = 0; i < n; i++) {
        int found = 0;
        for (j = 0; j < 3; j++) {
            if (frame[j] == pages[i]) {
                found = 1;
                time[j] = ++counter;
            }
        }
        if (!found) {
            pos = 0;
            for (j = 1; j < 3; j++)
                if (time[j] < time[pos]) pos = j;

            frame[pos] = pages[i];
            time[pos] = ++counter;
            faults++;
        }
    }
    printf("Page Faults = %d\n", faults);
    return 0;
}
