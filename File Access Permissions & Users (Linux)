#include <stdio.h>

int main() {
    char *users[] = {"Owner", "Owner", "Group", "Others"};
    char *permissions[] = {"Read", "Write", "Read", "Read"};
    int i;

    printf("File Access Permissions:\n");

    for (i = 0; i < 4; i++) {
        printf("%s: %s\n", users[i], permissions[i]);
    }

    return 0;
}
