#include <stdio.h>

int main() {
    int index[10], n, i;
    printf("Enter number of blocks: ");
    scanf("%d", &n);

    printf("Enter blocks: ");
    for (i = 0; i < n; i++)
        scanf("%d", &index[i]);

    printf("File blocks:\n");
    for (i = 0; i < n; i++)
        printf("%d -> %d\n", i, index[i]);

    return 0;
}
